extends ../layouts/master.pug

block title
	title Ecom - Marketplace Dashboard Template

block content
	.content-header
		a(href='javascript:history.back()')
			i.material-icons.md-arrow_back
			|  Go back
	.card.mb-4
		.card-header.bg-primary(style='height:150px')
		.card-body
			.row
				.col-xl.col-lg.flex-grow-0(style='flex-basis:230px')
					.img-thumbnail.shadow.w-100.bg-white.position-relative.text-center(style='height:190px; width:200px; margin-top:-120px')
						img.center-xy.img-fluid(src='backend/assets/imgs/brands/brand-3.jpg' alt='Logo Brand')
				// col.//
				.col-xl.col-lg
					h3 Cocorico sports shop
					p 3891 Ranchview Dr. Richardson, California 62639
				// col.//
				.col-xl-4.text-md-end
					select.form-select.w-auto.d-inline-block
						option Actions
						option Disable shop
						option Analyze
						option Something
					a.btn.btn-primary(href='#')
						| View live
						i.material-icons.md-launch
				// col.//
				// card-body.//
				hr.my-4
				.row.g-4
					.col-md-12.col-lg-4.col-xl-2
						article.box
							p.mb-0.text-muted Total sales:
							h5.text-success 238
							p.mb-0.text-muted Revenue:
							h5.text-success.mb-0 $2380
					// col.//
					.col-sm-6.col-lg-4.col-xl-3
						h6 Contacts
						p
							| Manager: Jerome Bell
							br
							|                                 info@example.com
							br
							|                                 (229) 555-0109, (808) 555-0111
					// col.//
					.col-sm-6.col-lg-4.col-xl-3
						h6 Address
						p
							| Country: California
							br
							|                                 Address: Ranchview Dr. Richardson
							br
							|                                 Postal code: 62639
					// col.//
					.col-sm-6.col-xl-4.text-xl-end
						map.mapbox.position-relative.d-inline-block
							img.rounded2(src='backend/assets/imgs/misc/map.jpg' alt='map' height='120')
							span.map-pin(style='top:50px; left: 100px')
							button.btn.btn-sm.btn-brand.position-absolute.bottom-0.end-0.mb-15.mr-15.font-xs  Large
